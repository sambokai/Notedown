{
  "name": "Notedown",
  "version": "1.0.0",
  "description": "A learning project using React without Redux.",
  "author": "Sam Bokai",
  "dependencies": {
    "codemirror": "^5.40.0",
    "moment": "^2.22.2",
    "mousetrap": "^1.6.2",
    "npm": "^6.4.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-beautiful-dnd": "^7.1.3",
    "react-codemirror2": "^5.1.0",
    "react-dom": "^16.5.1",
    "react-markdown": "^3.6.0",
    "react-router-dom": "^4.3.1",
    "remove-markdown": "^0.3.0",
    "scroll-into-view-if-needed": "^2.2.16"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^22.4.4",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chromedriver": "^2.41.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.4",
    "react-test-renderer": "^16.5.1",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "start-server-and-test": "^1.7.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "3.1.8"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test:unit -- --watchAll",
    "test:ci": "start-server-and-test dev http-get://localhost:8080 test",
    "test:unit": "jest --testPathIgnorePatterns='/test/e2e/'",
    "test:e2e": "jest --testPathPattern='/test/e2e/'",
    "build": "webpack",
    "dev": "webpack-dev-server"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test/jest.setup.js",
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/jest.setup.js",
      "!src/index.jsx"
    ],
    "clearMocks": true,
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    }
  }
}
