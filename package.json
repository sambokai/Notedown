    "react-test-renderer": "latest",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "start-server-and-test": "^1.4.1",
    "style-loader": "^0.21.0",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2"
